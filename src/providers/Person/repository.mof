class Person
{
    [key] uint32 ssn;
    string first;
    string last;
};

class Salesman : Person
{
    uint32 budget;
};

[Association]
class PersonLink
{
    [key] Person ref parent;
    [key] Person ref child;
};

[Association]
class SalesmanLink : PersonLink
{
