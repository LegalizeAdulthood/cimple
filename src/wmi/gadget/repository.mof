    string key;

    boolean b;
    uint8 u8;
    sint8 s8;
    uint8 u16;
    sint8 s16;
    uint8 u32;
    sint8 s32;
    uint8 u64;
    sint8 s64;
    real32 r32;
    real64 r64;
    datetime dt;

    boolean ba[];
    uint8 u8a[];
    sint8 s8a[];
    uint16 u16a[];
    sint16 s16a[];
    uint32 u32a[];
    sint32 s32a[];
    uint64 u64a[];
    sint64 s64a[];
    real32 r32a[];
    real64 r64a[];
    string sa[];
    datetime dta[];
};

