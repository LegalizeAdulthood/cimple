    [implemented, static]
    boolean foo(
        [in] uint32 arg1, 
        [in] string arg2, 
        [in(false), out] uint32 arg3,
        [in(false), out] string arg4);

    [implemented]
    uint32 goo(
        [in] Gadget REF gin,
        [in(false), out] Gadget REF gout);

    [implemented, static]
    uint32 hoo(
